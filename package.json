{
  "name": "bot-manager-multitenant",
  "version": "1.0.0",
  "description": "Plataforma Multi-Tenant de Bots WhatsApp con @builderbot/bot",
  "main": "dist/manager/index.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/manager/index.js",
    "manager": "node ./dist/manager/index.js",
    "worker": "node ./dist/worker/bot.js",
    "dev": "tsx watch ./src/manager/index.ts",
    "dev:manager": "tsx watch ./src/manager/index.ts",
    "dev:worker": "tsx ./src/worker/bot.ts",
    "build": "npm run build:manager && npm run build:worker",
    "build:manager": "npx rollup -c rollup.manager.config.js",
    "build:worker": "npx rollup -c rollup.worker.config.js",
    "lint": "eslint . --no-ignore",
    "clean": "rm -rf dist",
    "start:old": "node ./dist/app.js",
    "dev:old": "npm run lint && nodemon ./src/app.ts"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "multi-tenant",
    "builderbot"
  ],
  "dependencies": {
    "@builderbot/bot": "1.2.2",
    "@builderbot/provider-baileys": "1.2.2",
    "@types/express": "^5.0.0",
    "express": "^4.21.2",
    "queue-promise": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-builderbot": "latest",
    "nodemon": "^3.1.0",
    "rollup": "^4.10.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "",
  "license": "ISC"
}
